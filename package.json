{
  "name": "horizor-app",
  "version": "1.0.0",
  "description": "Official TypeScript for the Horizor API - AI-powered pre-launch blockchain project analysis on Solana",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "solana",
    "blockchain",
    "analytics",
    "ai",
    "nft",
    "tokens",
    "sdk",
    "horizor",
    "crypto",
    "investment",
    "risk-assessment"
  ],
  "author": "Horizor Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HorizorApp/horizor-app.git"
  },
  "bugs": {
    "url": "https://github.com/HorizorApp/horizor-app/issues"
  },
  "homepage": "https://github.com/HorizorApp/horizor-app#readme",
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.8.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}